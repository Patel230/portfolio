    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "terser": "^5.46.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
